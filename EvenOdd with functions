import java.util.*;
public class Hello{
    public void evenodd(int c){
        if(c%2==0){
        System.out.println("even");
    }
    else
    {
        System.out.println("odd");
    }
    }
    public static void main(String[] args)
    {
    Scanner sc=new Scanner(System.in);
    int c=sc.nextInt();
    Hello cn=new Hello();
    cn.evenodd(c);
    
    }
}
